{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "req",
      "direction": "in",
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "route": "xls/{template}/{document}"
    },
    {
      "name": "template",
      "direction": "in",
      "type": "blob",
      "path": "template/{template}",
      "connection": "xlsBlob"
    },
    {
      "name": "inblob",
      "direction": "in",
      "type": "blob",
      "path": "xls/{document}",
      "connection": "xlsBlob"
    },
    {
      "direction": "out",
      "type": "cosmosDB",
      "name": "outdoc",
      "databaseName": "xlsdb",
      "collectionName": "xlscol",
      "leaseCollectionName": "xlsdocs",
      "createLeaseCollectionIfNotExists": true,
      "connectionStringSetting": "xlsCosmosDb",
      "createIfNotExists": true
    }
  ]
}